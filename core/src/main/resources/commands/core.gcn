name = "arcadius"
description = "ArcadiusMC admin command"

literal("reload") {
  description = "Reloads the plugin"
  executes = reload()
}

literal("reload-config") {
  description = "Reloads the plugin config"
  executes = reloadConfig()
}

literal("save") {
  description = "Forces the plugin to save all data"
  executes = save()
}